{
  "name": "titanium-kitten-karaoke",
  "version": "1.0.0",
  "description": "Titanium Kitten Karaoke - Song Request System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "env:setup": "copy server.env .env && copy client\\client.env client\\.env"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "axios-cookiejar-support": "^6.0.4",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "i18next": "^25.5.2",
    "jsonwebtoken": "^9.0.2",
    "qrcode": "^1.5.4",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.6",
    "tough-cookie": "^5.1.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10"
  },
  "keywords": [
    "karaoke",
    "music",
    "playlist",
    "requests",
    "titanium-kitten"
  ],
  "author": "",
  "license": "MIT"
}
